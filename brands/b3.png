	//Item Add to Compare List Script
	jQuery("body").delegate('.compare-item .link-compare-btn:not(.added)','click', function(e) {
		e.preventDefault();
		var count = jQuery(".compare-features .compare-product").length;
		if(count<=3){
			var productId = jQuery(this).closest(".compare-item").find(".compareproductid").val();
			var productName = jQuery(this).closest(".compare-item").find(".compareproductname").html();
			var productImg = jQuery(this).closest(".compare-item").find(".compareproductimg").attr("src");
			jQuery.ajax({
				url: "https://www.buildersmart.in/vendorlisting/index/compareajax",
				data: { 
					proid: productId,
				},
				type: 'POST',
				async: false
			})
			.done(function(response) {
				count++;
				jQuery(".compare-features").append('<div class="compare-product selected">'+
					'<a href="https://www.buildersmart.in/catalog/product_compare/remove/product/'+productId+'" class="remove-selected btn-remove" title="Remove This Item"> Remove This Item</a>'+							
					'<div class="vs"><span>VS</span></div>'+
					'<div class="product-item">'+
						'<a href="#" target="_blank" title="Ultratech Super Cement">'+
							'<div class="product-image">'+
								'<img class="product-collection-image pt-p-s" src="'+productImg+'" alt="">'+
							'</div>'+
							'<div class="product-info">'+
								'<div class="product-name">'+productName+'</div>'+
							'</div>'+
						'</a>'+
					'</div>'+
				'</div>');
				jQuery(".compare-products-btn").show();
				jQuery(".compare-products-btn span").html(parseInt(count));
			});
			jQuery(this).addClass('added');
		}
		else{
			jQuery(".show-compare-error").show();
			setTimeout(function(){jQuery(".show-compare-error").hide();}, 4000);
		}
   });
   
	//Item Remove from Compare List Script
	jQuery("body").delegate('.compare-item .link-compare-btn.added','click', function(e) {
		e.preventDefault();
		var count = jQuery(".compare-features .compare-product").length;
		var productId = jQuery(this).closest(".compare-item").find(".compareproductid").val();
		var productName = jQuery(this).closest(".compare-item").find(".compareproductname").html();
		var productImg = jQuery(this).closest(".compare-item").find(".compareproductimg").attr("src");

		jQuery.ajax({
			url: "https://www.buildersmart.in/catalog/product_compare/remove/product/"+productId,
				data: { 
				proid: productId,
			},
			type: 'POST',
			async: false
		})
		.done(function(response) {
			--count;
			jQuery(".compare-features .compare-product ").each(function(){
				var ProName1 = jQuery(this).find('.product-name').html();
				if(ProName1 == productName){
					jQuery(this).remove();
				}
			});
			jQuery(".compare-products-btn").show();
			jQuery(".compare-products-btn span").html(parseInt(count));
			if(count<1){
				jQuery(".compare-products-btn").hide();
			}
			else{
				jQuery(".compare-products-btn").show();
			}
		});
		jQuery(this).removeClass('added');
   });

	//Hide Error Message 

	jQuery("body").delegate('.show-compare-error','click', function(e) {
		jQuery(this).hide();
	});
	
   var count = "0";
