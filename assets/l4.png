		        url: "https://www.buildersmart.in/sellerpricing/index/joinoffersotp",
		        type: 'POST',
		        data: {
		        	tel:telephone
		        },
		    }).done( function(response) {
		    	jQuery('#Gen-OTP').val(response);
			});
		}
		else{
			jQuery(".show-verify-block").hide();
			jQuery(".mobileno").show();
		}
	});

	jQuery("body").delegate(".new-popup-close, .new-popup-footer a","click",function(){
		jQuery(".new-popup-modal").hide();
		sessionStorage.setItem('popup','hide');
	});

