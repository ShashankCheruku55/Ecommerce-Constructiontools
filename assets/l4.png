	//Item Add to Compare List Script
	jQuery("body").delegate('.compare-item .link-compare-btn:not(.added)','click', function(e) {
		e.preventDefault();
		var count = jQuery(".compare-features .compare-product").length;
		if(count<=3){
			var productId = jQuery(this).closest(".compare-item").find(".compareproductid").val();
			var productName = jQuery(this).closest(".compare-item").find(".compareproductname").html();
			var productImg = jQuery(this).closest(".compare-item").find(".compareproductimg").attr("src");
			jQuery.ajax({
				url: "https://www.buildersmart.in/vendorlisting/index/compareajax",
				data: { 
					proid: productId,
				},
				type: 'POST',
				async: false
			})
			.done(function(response) {
				count++;
				jQuery(".compare-features").append('<div class="compare-product selected">'+
					'<a href="https://www.buildersmart.in/catalog/product_compare/remove/product/'+productId+'" class="remove-selected btn-remove" title="Remove This Item"> Remove This Item</a>'+							
					'<div class="vs"><span>VS</span></div>'+
					'<div class="product-item">'+
						'<a href="#" target="_blank" title="Ultratech Super Cement">'+
							'<div class="product-image">'+
